{
  "name": "web-ext-msg-passing",
  "version": "0.0.1",
  "description": "simple web extension message passing library",
  "type": "module",
  "main": "dist/index.cjs",
  "source": "src/index.ts",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.modern.js"
    }
  },
  "unpkg": "./dist/index.umd.js",
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch"
  },
  "keywords": [
    "browser",
    "extension",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/ckaznable/web-ext-msg-passing"
  },
  "files": [
    "dist",
    "types/index.d.ts",
    "src",
    "README.md",
    "CHANGELOG.md"
  ],
  "author": "kami25565@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "microbundle": "^0.15.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@types/chrome": "^0.0.188"
  }
}
